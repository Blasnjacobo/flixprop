{
    "build": {
      "builder": "nixpacks",
      "nixpacksConfig": {
        "phases": {
          "install": {
            "commands": [
              "npm ci"
            ]
          },
          "build": {
            "commands": [
              "npm run build"
            ]
          },
          "start": {
            "commands": [
              "npm start"
            ]
          }
        }
      }
    }
  }
  